# Production Environment Variables
# Copy this to .env.production and update values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_PUBLISHABLE_KEY=your-supabase-publishable-key

# ML API URL - Hugging Face Spaces
# Get this URL after deploying ML backend to Hugging Face Spaces
# Example: VITE_ML_API_URL=https://your-username-census-ml.hf.space/extract
VITE_ML_API_URL=https://your-hf-space-url/extract

# Analytics (optional)
VITE_GA_ID=

